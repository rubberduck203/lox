{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/lox/src/bin/Debug/net5.0/lox.dll",
            "args": [],
            "cwd": "${workspaceFolder}/lox/src",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "fibonacci",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/lox/src/bin/Debug/net5.0/lox.dll",
            "args": ["${workspaceFolder}/programs/fibonacci.lox"],
            "cwd": "${workspaceFolder}/lox/src",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Debug with Lox File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/lox/src/bin/Debug/net5.0/lox.dll",
            "args": ["${workspaceFolder}/${relativeFile}"],
            "cwd": "${workspaceFolder}/lox/src",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "AST Printer",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/astprinter/bin/Debug/net5.0/astprinter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/astprinter/",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}